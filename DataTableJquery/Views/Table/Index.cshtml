@{
    ViewData["Title"] = "Index";
	Layout = "_Layout";
}


@{
	ViewData["Title"] = "GetCustomerList";
}

<div class="container">
	<a class="btn btn-sm btn-primary m-2" asp-action="" asp-controller="Table">Upload File</a>
	<h3 class="text-center text-success font-weight-bold">@TempData["message"]</h3>
	<div class="row">
		<div class="col-8 offset-2">
			<table class="table table-bordered" id="DataTableData">
				<thead>
					<tr class="text-center">
						<th>id</th>
						<th>firstname</th>
						<th>lastname</th>
						<th>gender</th>
						<th>country</th>
						<th>age</th>
					</tr>
				</thead>
			</table>
		</div>
	</div>
</div>

@section scripts {

					<script src="~/js/datatablefile.js"></script>

}